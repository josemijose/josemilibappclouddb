<!-- navbar ends -->

<!-- form begins -->

  <form action="books"  onsubmit="return fname()" > 
    <div class="signup">
      <h2>Signup to Continue</h2>
        <br>
          <label for="fname" class="col-sm-4 col-form-label">First name</label>
          <input type="text" class="form-control" placeholder="First name" id="firstname" required >
          <br>
          <label for="fname" class="col-sm-4 col-form-label">Last name</label>
          <input type="text" class="form-control" placeholder="Last name" id="lastname" required > 
      <br>
        <label for="exampleInputEmail" class="col-sm-4 col-form-label">Email address</label>
        <input type="email" class="form-control" id="email" aria-describedby="emailHelp" placeholder="Enter email" required>
      <br>
        <label for="phone" class="col-sm-4 col-form-label">Mob No</label>
        <input type="tel" class="form-control" id="mobilenumber"  placeholder="Enter Mobile No" onkeyup="return fmob()" required>
        <label id="belowmob" class=""></label>
      <br>
        <label for="exampleInputPassword1"  class="col-sm-4 col-form-label">Password</label>
        <input type="password" class="form-control" id="password" onkeyup="return fpass()" placeholder="Password" required>
        <label id="belowpass"></label>
      <br>
       <label for="exampleInputPassword1" class="col-sm-4 col-form-label">Confirm Password</label>
        <input type="password" class="form-control" id="password1" onkeyup="return fpass1()" placeholder="Confirm Password" required>
        <label id="belowpass1" ></label>
      <br><br> <br>
        <button type="submit" class="btn btn-primary">Sign Up</button>
    </div>
      
  </form>

  <!-- /form -->

